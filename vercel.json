{
  "version": 2,
  "builds": [{"src": "portfolio/wsgi.py", "use": "@vercel/python"}],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1",
      "headers": {"Cache-Control": "public, max-age=31536000"}
    },
    {
      "src": "/(.*)",
      "dest": "portfolio/wsgi.py"
    }
  ]
}
