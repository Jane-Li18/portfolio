{
  "version": 2,
  "builds": [{
    "src": "portfolio/wsgi.py",
    "use": "@vercel/python",
    "config": {
      "includeFiles": ["staticfiles/**", "portfolioapp/static/**"]
    }
  }],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "portfolio/wsgi.py",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "portfolio/wsgi.py"
    }
  ]
}
